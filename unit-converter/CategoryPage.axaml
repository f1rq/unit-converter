<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="380"
        x:Class="unit_converter.CategoryPage">
        
        <Grid
                Margin="40 20"
                RowSpacing="30"
                ColumnSpacing="20"
        >
                <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                
                <TextBlock Grid.Row="0" Text="Unit Converter" FontSize="22" FontWeight="SemiBold" HorizontalAlignment="Center"/>

                <UniformGrid x:Name="CatBtnStackPanel" Grid.Row="1" Margin="20" ColumnSpacing="30" RowSpacing="30" Rows="2" Columns="3" HorizontalAlignment="Center"/>
        </Grid>
        
        
</UserControl>
